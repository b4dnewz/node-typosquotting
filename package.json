{
  "name": "typosquotter",
  "version": "0.0.3",
  "description": "A NodeJS tool for generating typosquotted domains",
  "homepage": "https://github.com/b4dnewz/node-typosquotting",
  "author": {
    "name": "b4dnewz",
    "email": "filippo@codekraft.it",
    "url": "https://b4dnewz.github.io/"
  },
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "bin": {
    "typosquotter": "lib/cli.js"
  },
  "browser": "dist/main.js",
  "keywords": [
    "nodejs",
    "typosquotting"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "lint-staged": "^7.0.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "lint": "eslint --fix .",
    "build": "webpack"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "repository": "b4dnewz/typosquotter",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "meow": "^5.0.0"
  }
}
