{
  "name": "typosquotter",
  "version": "0.0.6",
  "description": "A NodeJS tool for generating typosquotted domains",
  "homepage": "https://github.com/b4dnewz/node-typosquotting",
  "author": {
    "name": "b4dnewz",
    "email": "filippo@codekraft.it",
    "url": "https://b4dnewz.github.io/"
  },
  "license": "MIT",
  "repository": "b4dnewz/node-typosquotting",
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "browser": "dist/main.js",
  "keywords": [
    "nodejs",
    "typosquotting"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prebuild": "npm run test",
    "test": "jest --coverage",
    "lint": "eslint --fix .",
    "build": "webpack"
  },
  "dependencies": {
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "lint-staged": "^8.1.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
